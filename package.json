{
  "name": "check",
  "version": "1.0.0",
  "description": "Commands that allow markdown checkboxes to be manipulated from the command line",
  "main": "index.js",
  "bin": {
    "check": "./bin/check.js"
  },
  "scripts": {
    "build": "webpack",
    "start": "ts-node --files -r tsconfig-paths/register src/index.ts",
    "prepare": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "eslint --ignore-path .eslintignore --ext .js,.ts . --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "keywords": [],
  "author": "ysnbogt",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "prettier": "^2.8.4",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "enquirer": "^2.3.6",
    "tsconfig-paths": "^4.1.2"
  }
}
